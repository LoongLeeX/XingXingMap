{
  "name": "google-maps-marker-app",
  "version": "0.1.0",
  "private": true,
  "description": "A full-featured Google Maps application with marker management, search, and 2D/3D comparison views",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "typecheck": "tsc --noEmit",
    "postinstall": "test \"$VERCEL\" = \"1\" && prisma generate --schema=./prisma/schema.production.prisma || prisma generate",
    "vercel-build": "next build"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.2",
    "@prisma/client": "^5.22.0",
    "clsx": "^2.1.0",
    "next": "14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.10.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.4.33",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "22.x"
  }
}
